<% if notice != nil %>
<div class="alert alert-success"> 
    <p id="notice"><%= notice %></p>
</div>  
<% end %>

<p>
  <b>Número: </b><%= @establecimiento.numero %>
</p>

<p>
  <b>Nombre: </b><%= @establecimiento.nombre %>
</p>

<p>
  <b>Dirección: </b><%= @establecimiento.direccion %>
</p>

<p>
  <b>CUE: </b><%= @establecimiento.cue %>
</p>

<p>
  <b>Zona: </b><%= @establecimiento.zona %>
</p>

<p>
  <b>Ubicacion cargos: </b><%= @establecimiento.ubicacion_cargos %>
</p>

<p>
  <b>Ubicacion horas: </b><%= @establecimiento.ubicacion_horas %>
</p>

<p>
  <b>Localidad: </b><%= Localidad.find(@establecimiento.localidad_id).nombre %>
