<% if notice != nil %>
<div class="alert alert-success"> 
    <p id="notice"><%= notice %></p>
</div>  
<% end %>

<p>
  <b>DNI: </b><%= @agente.dni %>
</p>

<p>
  <b>Apellido: </b><%= @agente.apellido %>
</p>

<p>
  <b>Nombre: </b><%= @agente.nombre %>
</p>

<p>
  <b>CUIT/CUIL: </b><%= @agente.cuit_cuil %>
</p>

<p>
  <b>Fecha ingreso: </b><%= @agente.fecha_ingreso %>
</p>

<p>
  <b>Localidad: </b><%= Localidad.find(@agente.localidad_id).nombre %>
</p>


